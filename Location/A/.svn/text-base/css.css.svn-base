*{
	margin: 0px;
	padding: 0px;
}
img{
	margin: 0 3px -3px 0;
}
html{
	overflow-x: hidden;
					
}
body{
	width: 1349px;	
	height: 900px;
	background-size: 100%;
	background-image:url(../Location_img/E.jpg);
	position: relative;
	background-repeat:no-repeat;
}
.Location_box{
	width: 1349px;	
	height:900px;
	background-color: rgba(255,255,255,0.2);
	position: absolute;
	left: 0px;
	top: 0px;
}
